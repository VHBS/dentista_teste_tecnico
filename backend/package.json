{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "npm install && npm run build",
    "start": "node build/index.js",
    "build": "npx tsc",
    "build:prod": "npx tsc -p ./tsconfig.build.json",
    "predev": "npm run build:prod",
    "dev": "tsnd --transpile-only --watch \"./src/**\" ./src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VHBS/dentista_teste_tecnico.git"
  },
  "author": "Victor Hugo Baum Schlichting",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VHBS/dentista_teste_tecnico/issues"
  },
  "homepage": "https://github.com/VHBS/dentista_teste_tecnico#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.1",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "sequelize-cli": "^6.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mysql2": "^2.3.3",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.4"
  }
}
